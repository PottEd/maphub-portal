<div class="row">
  <div class="span12">
  
  <h1>Search Results <small>Your search for "<%= @q %>" returned <%= @search.results.length %> results.</small></h1>
  
  <% if @results.length > 0 %>
    <div class="row">
      <div class="span12">
        <table class="table table-striped">
        <% for map in @results %>
          <%= render :partial => "maps/table_row", :locals => { :map => map } %>
        <% end %>
        </table>
      </div>
    </div>
  <% end %>
  
  </div>
</div>
<div class="row">
    <div class="span12"><center><%= paginate @results %></center></div>
</div>